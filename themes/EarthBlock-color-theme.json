{
	"name": "EarthBlock",
	"colors": {
		"editor.background": "#56443b",
		"editor.foreground": "#e9e3d3",
		"activityBar.background": "#29211d",
		"activityBar.foreground": "#e9e3d3",
		"activityBarBadge.background": "#81af83",
		"activityBarBadge.foreground": "#e9e3d3",
		"sideBar.background": "#29211d",
		"sideBar.foreground": "#e9e3d3",
		"sideBarTitle.foreground": "#e9e3d3",
		"sideBarSectionHeader.background": "#29211d",
		"sideBarSectionHeader.foreground": "#e9e3d3",
		"tab.activeBackground": "#56443b",
		"tab.activeForeground": "#e9e3d3",
		"tab.inactiveBackground": "#3b2f29",
		"tab.inactiveForeground": "#8c8c8c",
		"editorGroupHeader.tabsBackground": "#29211d",
		"titleBar.activeBackground": "#29211d",
		"titleBar.activeForeground": "#e9e3d3",
		"titleBar.inactiveBackground": "#4c3c35",
		"titleBar.inactiveForeground": "#8c8c8c",
		"statusBar.background": "#29211d",
		"statusBar.foreground": "#e9e3d3",
		"button.background": "#81af83",
		"button.foreground": "#e9e3d3",
		"button.hoverBackground": "#8abb8d",
		"errorForeground": "#eb5149",
		"editorError.foreground": "#eb5149",
		"editorWarning.foreground": "#cd7a2c",
		"editorInfo.foreground": "#479eca",
		"editorHint.foreground": "#43f68b",
		"editor.lineHighlightBorder": "#81af83",
		"editor.lineHighlightBackground": "#56443b",
		"textLink.foreground": "#81af83",
		"textLink.activeForeground": "#8abb8d",
		"dropdown.background": "#3b2f29",
		"dropdown.foreground": "#e9e3d3",
		"inputOption.activeBorder": "#77a27a",
		"input.background": "#5a4b45",
		"input.foreground": "#e9e3d3",
		"inputOption.activeBackground": "#3b2f29",
		"editorWidget.background": "#3b2f29",
		"editorWidget.foreground": "#e9e3d3",
		"input.border": "#77a27a",
		"focusBorder": "#77a27a",
		"inputOption.activeForeground": "#e9e3d3",
		"inputOption.hoverBackground": "#77a27a",
		"editorHoverWidget.background": "#3b2f29",
		"editorHoverWidget.foreground": "#e9e3d3",
		"editorHoverWidget.border": "#77a27a",
		"quickInputList.focusBackground": "#81af83",
		"quickInputList.focusForeground": "#e9e3d3",
		"list.hoverBackground": "#3b453c",
		"list.hoverForeground": "#e9e3d3",
		"dropdown.listBackground": "#3b2f29",
		"pickerGroup.foreground": "#81af83"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Syntax",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#81af83"
			}
		},
		{
			"name": "Tags and Invalid",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter",
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#eb5149"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#479eca"
			}
		},
		{
			"name": "Variables and Strings",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"constant.other.color",
				"meta.block variable.other",
				"support.other.variable",
				"string.other.link",
				"string.other.link.title.markdown",
				"string.other.link.description.title.markdown",
				"constant.other.reference.link.markdown",
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e9e3d3"
			}
		},
		{
			"name": "Numbers and Constants",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cd7a2c"
			}
		},
		{
			"name": "Classes and Types",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7543f6"
			}
		},
		{
			"name": "CSS Properties",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#81af83"
			}
		},
		{
			"name": "Attributes and Classes",
			"scope": [
				"entity.other.attribute-name",
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name",
				"entity.other.attribute-name.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7543f6"
			}
		},
		{
			"name": "Button Text",
			"scope": ["button"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e9e3d3"
			}
		},
		{
			"name": "Dropdown Items",
			"scope": ["dropdown"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e9e3d3"
			}
		},
		{
			"name": "Markdown and Git Gutter",
			"scope": [
				"markup.inserted",
				"markup.deleted",
				"markup.changed"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#75fb59"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#479eca"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cd7a2c"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#81af83"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#479eca"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#eb5149"
			}
		}
	]
}
